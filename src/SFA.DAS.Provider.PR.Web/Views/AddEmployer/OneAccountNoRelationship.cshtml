@using SFA.DAS.Provider.PR.Web.Infrastructure
@using Microsoft.AspNetCore.Mvc.TagHelpers
@model SFA.DAS.Provider.PR.Web.Models.AddEmployer.OneAccountNoRelationshipViewModel
@{
    ViewData["Title"] = "account found - no relationship";
}
@section breadcrumb {
    <a href="@Model.BackLink" class="govuk-back-link">Back</a>
}
<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <h1 class="govuk-heading-l">
            Add Contact
        </h1>
    </div>
    <div class="govuk-grid-column-two-thirds">
        <p class="govuk-body">
            We’ve found an apprenticeship service account linked to @Model.Email.<br/>
            You can invite them to add you as a training provider and request permissions.        
            </p>
        <div class="govuk-button-group">
            <a href="@Model.ContinueLink" role="button" draggable="false" class="govuk-button" data-module="govuk-button">
                Continue
            </a>
            <a class="govuk-link" href="@Model.CancelLink">Cancel</a>
        </div>
    </div>
</div>